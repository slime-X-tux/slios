{
  "version": "0.2.0",
  "configurations": [

    {
      "type": "lldb",
      "request": "custom",
      "name": "Custom launch",
      "targetCreateCommands": [
        "target create ${workspaceFolder}/<program>"
      ],
      "processCreateCommands": [
        "settings set target.run-args value1 value2 value3",
        "process launch"
      ]
    },
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug",
      "program": "${workspaceFolder}/slios/install/code/py/week-4/filter-more/filter",
      "args": [],
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/si/slios/install/code/py/week-4/filter-more",
      "program": "/home/si/slios/install/code/py/week-4/filter-more/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}